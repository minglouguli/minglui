{
  "name": "minglui",
  "version": "1.0.0",
  "private": false,
  "main": "./src/packages/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:Test": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:Main": "vue-cli-service build --dest ./lib/main  --target lib --name core --report ./src/packages/index.js",
    "build:BackendPackage": "vue-cli-service build --dest ./lib/backendPackage --target lib --name el ./src/packages/backendPackage.js",
    "build:MlCodeEditor": "vue-cli-service build --dest ./lib/mlCodeEditor --target lib --name sax ./src/packages/ml-code-editor.js",
    "build:All": "npm run build:Main && npm run build:BackendPackage && npm run build:MlCodeEditor"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^3.2.8",
    "codemirror": "^5.60.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.6",
    "minglui-core": "^1.0.0",
    "plupload": "^2.3.7",
    "tinymce": "^5.10.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuesax": "^4.0.1-alpha.25",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
